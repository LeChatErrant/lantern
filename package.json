{
  "name": "templated-project-cli",
  "version": "3.0.0",
  "description": "CLI to assist development of API-template, devops-template and back-office-template",
  "bin": {
    "templated-project-cli": "bin/src/index.js"
  },
  "files": [
    "bin/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "tsc",
    "start": "node bin/src",
    "dev": "ts-node src",
    "lint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:ci": "eslint './src/**/*.{ts,tsx}'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeChatErrant/templated-project-cli.git"
  },
  "keywords": [
    "cli",
    "templated"
  ],
  "author": {
    "name": "LeChatErrant",
    "email": "guillaume.hector@epitech.eu",
    "url": "https://github.com/LeChatErrant/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeChatErrant/templated-project-cli/issues"
  },
  "homepage": "https://github.com/LeChatErrant/templated-project-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "commander": "^8.3.0",
    "figlet": "^1.5.0",
    "inquirer": "^8.2.0",
    "np": "^7.4.0",
    "npm": "^7.6.3",
    "pluralize": "^8.0.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/clear": "^0.1.1",
    "@types/clui": "^0.3.0",
    "@types/figlet": "^1.2.1",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.35",
    "@types/npm": "^2.0.31",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "ts-node": "^9.1.1"
  }
}
